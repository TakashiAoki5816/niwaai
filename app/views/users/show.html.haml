= render partial: "users/header"
.Contents
  - @post.each do |post|
    .Contents-box
      .Contents-box__header
        .title
          = post.garden_name
      .Contents-box__user
        .user-image
          = image_tag "pict/IMG_4608.jpeg", size: "50x50"
        .username
          = post.user.name
      .Contents-box__titlebox
        .title
          = post.title
      .Contents-box__imagebox
        .image
          = image_tag post.image.url, size: "330x260"
      .Contents-box__footer
        .like
          いいね！
        .comment
          コメント1件
